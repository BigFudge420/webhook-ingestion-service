generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model WebhookEvent {
  id                Int      @id @default(autoincrement())
  orderId           String
  status            String
  providerTimeStamp DateTime
  providerEventId   String   @unique
  createdAt         DateTime @default(now())

  @@index([orderId, createdAt(sort: Desc)])
}
