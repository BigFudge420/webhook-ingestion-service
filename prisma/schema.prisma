datasource db {
  provider = "postgresql" 
}

generator client {
  provider = "prisma-client-js"
}

model WebhookEvent {
  id Int @id @default(autoincrement())
  orderId String
  status String
  providerTimeStamp DateTime
  providerEventId String @unique
  createdAt DateTime @default(now())

  @@index([orderId, createdAt(sort : Desc)])
}